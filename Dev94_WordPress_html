					<div class="screenshot"><img src="{{ data.screenshot[0] }}?ver={{ data.version }}" alt="" /></div>
				<# } else { #>
					<div class="screenshot blank"></div>
				<# } #>
				</div>

				<div class="theme-info">
					<# if ( data.active ) { #>
						<span class="current-label">Tema activo</span>
					<# } #>
					<h2 class="theme-name">{{{ data.name }}}<span class="theme-version">
						Versión: {{ data.version }}					</span></h2>
					<h3 class="theme-author">
						Por {{{ data.authorAndUri }}}					</h3>

					<# if ( data.stars && 0 != data.num_ratings ) { #>
						<div class="theme-rating">
							{{{ data.stars }}}
							<a class="num-ratings" target="_blank" href="{{ data.reviews_url }}">
								({{ data.num_ratings }} valoraciones) <span class="screen-reader-text">(abre en una nueva pestaña)</span>							</a>
						</div>
					<# } #>

					<# if ( data.hasUpdate ) { #>
						<# if ( data.updateResponse.compatibleWP && data.updateResponse.compatiblePHP ) { #>
							<div class="notice notice-warning notice-alt notice-large" data-slug="{{ data.id }}">
								<h3 class="notice-title">Actualización disponible</h3>
								{{{ data.update }}}
							</div>
						<# } else { #>
							<div class="notice notice-error notice-alt notice-large" data-slug="{{ data.id }}">
								<h3 class="notice-title">Actualización incompatible</h3>
								<p>
									<# if ( ! data.updateResponse.compatibleWP && ! data.updateResponse.compatiblePHP ) { #>
										Está disponible una nueva versión de {{{ data.name }}}, pero no funciona con tus versiones de WordPress y PHP. <a href="http://localhost/wordpress/wp-admin/update-core.php">Actualizá WordPress</a> y después <a href="https://wordpress.org/support/update-php/">aprendé más sobre cómo actualizar PHP</a>.									<# } else if ( ! data.updateResponse.compatibleWP ) { #>
										Hay una nueva versión de {{{ data.name }}} disponible, pero no funciona con tu versión de WordPress. <a href="http://localhost/wordpress/wp-admin/update-core.php">Actualizá WordPress</a>.									<# } else if ( ! data.updateResponse.compatiblePHP ) { #>
										Hay una nueva versión de {{{ data.name }}} disponible, pero no funciona con tu versión de PHP. <a href="https://wordpress.org/support/update-php/">Aprendé más sobre cómo actualizar PHP</a>.									<# } #>
								</p>
							</div>
						<# } #>
					<# } #>

					<# if ( data.parent ) { #>
						<p class="parent-theme">
							Este es un tema hijo de <strong>{{{ data.parent }}}</strong>.						</p>
					<# } #>

					<# if ( ! data.compatibleWP || ! data.compatiblePHP ) { #>
						<div class="notice notice-error notice-alt notice-large"><p>
							<# if ( ! data.compatibleWP && ! data.compatiblePHP ) { #>
								Este tema no funciona con tus versiones de WordPress y PHP. <a href="http://localhost/wordpress/wp-admin/update-core.php">Actualizá WordPress</a> y después <a href="https://wordpress.org/support/update-php/">aprendé más sobre cómo actualizar PHP</a>.							<# } else if ( ! data.compatibleWP ) { #>
								Este tema no funciona con tu versión de WordPress. <a href="http://localhost/wordpress/wp-admin/update-core.php">Actualizá WordPress</a>.							<# } else if ( ! data.compatiblePHP ) { #>
								Este tema no funciona con tu versión de PHP. <a href="https://wordpress.org/support/update-php/">Aprendé más sobre cómo actualizar PHP</a>.							<# } #>
						</p></div>
					<# } else if ( ! data.active && data.blockTheme ) { #>
						<div class="notice notice-error notice-alt notice-large"><p>
						Este tema no es compatible con el personalizador.						<# if ( data.actions.activate ) { #>
							 Sin embargo todavía podés <a href="{{{ data.actions.activate }}}">activar este tema</a> y usar el editor del sitio para personalizarlo.						<# } #>
						</p></div>
					<# } #>

					<p class="theme-description">{{{ data.description }}}</p>

					<# if ( data.tags ) { #>
						<p class="theme-tags"><span>Etiquetas:</span> {{{ data.tags }}}</p>
					<# } #>
				</div>
			</div>

			<div class="theme-actions">
				<# if ( data.active ) { #>
					<button type="button" class="button button-primary customize-theme">Personalizar</button>
				<# } else if ( 'installed' === data.type ) { #>
											<# if ( data.actions && data.actions['delete'] ) { #>
							<a href="{{{ data.actions['delete'] }}}" data-slug="{{ data.id }}" class="button button-secondary delete-theme">Borrar</a>
						<# } #>
					
					<# if ( data.blockTheme ) { #>
												<# if ( data.compatibleWP && data.compatiblePHP && data.actions.activate ) { #>
							<a href="{{{ data.actions.activate }}}" class="button button-primary activate" aria-label="Activar {{ data.name }}">Activar</a>
						<# } #>
					<# } else { #>
						<# if ( data.compatibleWP && data.compatiblePHP ) { #>
							<button type="button" class="button button-primary preview-theme" data-slug="{{ data.id }}">Vista previa</button>
						<# } else { #>
							<button class="button button-primary disabled">Vista previa</button>
						<# } #>
					<# } #>
				<# } else { #>
					<# if ( data.compatibleWP && data.compatiblePHP ) { #>
						<button type="button" class="button theme-install" data-slug="{{ data.id }}">Instalar</button>
						<button type="button" class="button button-primary theme-install preview" data-slug="{{ data.id }}">Instalar y previsualizar</button>
					<# } else { #>
						<button type="button" class="button disabled">No se puede instalar</button>
						<button type="button" class="button button-primary disabled">Instalar y previsualizar</button>
					<# } #>
				<# } #>
			</div>
		</div>
	</script>
	
		<script type="text/html" id="tmpl-media-frame">
		<div class="media-frame-title" id="media-frame-title"></div>
